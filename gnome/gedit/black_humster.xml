<?xml version="1.0" encoding="UTF-8"?>

<style-scheme id="black_humster" _name="Black Humster" version="1.0">
  <author>Pawel Chomicki</author>
  <_description>Experimental</_description>

  <color name="yellow"       value="#dad439"/>
  <color name="purple"       value="#9a77d1"/>
  <color name="red"          value="#d34f46"/>
  <color name="green"        value="#43c14e"/>
  <color name="orange"       value="#d89418"/>
  <color name="light-blue"   value="#5baeb6"/>
  <color name="light-orange"   value="#DED7C1"/>
  <color name="middle-blue"  value="#C2BEE6"/>
 
  <style name="docplan:section"            foreground="yellow"/>
  <style name="docplan:testname"           foreground="purple" bold="true"/>
  <style name="docplan:passed"             foreground="green"/>
  <style name="docplan:failed"             foreground="red"/>
  <style name="docplan:specialchars"       foreground="light-blue" bold="true"/>
  <style name="docplan:inbrackets"         foreground="light-blue"/>
  <style name="docplan:numbers"            foreground="orange"/>
  <style name="docplan:important"          foreground="red"/>
  <style name="docplan:work"               foreground="middle-blue"/>
  <style name="docplan:new"                foreground="light-orange"/>
      
  <style name="css:at-rules"               foreground="#cc7833"/>
  <style name="css:color"                  foreground="#6d9cbe"/>
  <style name="css:keyword"                foreground="#6e9cbe"/>

  <style name="bracket-match"              foreground="#e9da20" background="#222324" bold="true"/>
  <style name="bracket-mismatch"           foreground="#e9da20" background="#222324" underline="true" bold="true"/>
  <style name="current-line"               background="#222324"/>
  <style name="cursor"                     foreground="#ffffff"/>
  <style name="draw-spaces"                foreground="#404040"/>
  <style name="line-numbers"               foreground="#e6e1dc" background="#333435"/>
  <style name="search-match"               background="#1a1a1a" bold="true" underline="true"/>
  <style name="selection"                  foreground="#e6e1dc" background="#545d73"/>
  <style name="text"                       foreground="#e6e1dc" background="#141414"/>

  <style name="def:base-n-integer"         foreground="#a5c261"/>
  <style name="def:boolean"                foreground="#6e9cbe"/>
  <style name="def:builtin"                foreground="#6e9cbe"/>
  <style name="def:character"              foreground="#a5c261"/>
  <style name="def:comment"                foreground="#808080" italic="true"/>
  <style name="def:complex"                foreground="#a5c261"/>
  <style name="def:constant"               foreground="#6d9cbe"/>
  <style name="def:decimal"                foreground="#a5c261"/>
  <style name="def:doc-comment"            foreground="#bc9458" italic="true"/>
  <style name="def:doc-comment-element"    foreground="#bc9458" italic="true"/>
  <style name="def:error"                  foreground="#ffffff" background="#990000"/>
  <style name="def:floating-point"         foreground="#a5c261"/>
  <style name="def:function"               foreground="#dad439"/> <!--#7fba29-->
  <style name="def:identifier"             foreground="#d0d0ff"/>
  <style name="def:keyword"                foreground="#cc7833" bold="true"/>
  <style name="def:note"                   foreground="#bc9458" italic="true"/>
  <style name="def:number"                 foreground="#a5c261"/>
  <style name="def:operator"               foreground="#cc7833" bold="true"/>
  <style name="def:preprocessor"           foreground="#a5c261"/>
  <style name="def:reserved"               foreground="#cc7833" bold="true"/>
  <style name="def:shebang"                foreground="#bc9458" italic="true"/>
  <style name="def:special-char"           foreground="#519f50"/>
  <style name="def:special-constant"       foreground="#6e9cbe"/>
  <style name="def:statement"              foreground="#cc7833" bold="true"/>
  <style name="def:string"                 foreground="#a5d03f"/>
  <style name="def:type"                   foreground="#6e9cbe"/>

  <style name="diff:added-line"            foreground="#e6e1dc" background="#144212"/>
  <style name="diff:location"              background="#2f33ab"/>
  <style name="diff:removed-line"          foreground="#e6e1dc" background="#660000"/>

  <style name="html:dtd"                   foreground="#a5c261"/>
  <style name="html:tag"                   foreground="#e8bf6a"/>

  <style name="js:function"                foreground="#da4939"/>

  <style name="perl:builtin"               foreground="#ffc66d"/>
  <style name="perl:include-statement"     foreground="#cc7833"/>
  <style name="perl:line-directive"        foreground="#6d9cbe"/>
  <style name="perl:special-variable"      foreground="#da4939"/>
  <style name="perl:type"                  foreground="#6d9cbe"/>
  <style name="perl:variable"              foreground="#d0d0ff"/>

  <style name="php:string"                 foreground="#a5c261"/>

  <style name="python:builtin-constant"    foreground="#cc7833" bold="true"/>
  <style name="python:builtin-function"    foreground="#ffc66d"/>
  <style name="python:builtin-object"      foreground="#6d9cbe"/>
  <style name="python:module-handler"      foreground="#cc7833" bold="true"/>
  <style name="python:special-variable"    foreground="#cc7833" bold="true"/>

  <style name="ruby:attribute-definition"  foreground="#da4939" bold="true"/>
  <style name="ruby:block-parameter"       foreground="#8aa6c1" italic="true"/>
  <style name="ruby:builtin"               foreground="#da4939" bold="true"/>
  <style name="ruby:support-constant"      foreground="#da4939"/>
  <style name="ruby:class-inherit"         foreground="#ffffff" italic="true"/>
  <style name="ruby:class-name"            foreground="#DA4939" bold="true"/><!--#8aa6c1-->
  <style name="ruby:class-variable"        foreground="#d0d0ff"/>
  <style name="ruby:complex-interpolation" foreground="#ca88ae"/> <!-- #519f50 -->
  <style name="ruby:constant"              foreground="#da4939"/>
  <style name="ruby:global-variable"       foreground="#ffc66d" bold="true"/>
  <style name="ruby:instance-variable"     foreground="#86aedd"/>
  <style name="ruby:module-handler"        foreground="#cc7833" bold="true"/>
  <style name="ruby:module-name"           foreground="#ffffff" bold="true"/>
  <style name="ruby:predefined-variable"   foreground="#6e9cbe" bold="true"/>
  <style name="ruby:rails"                 foreground="#da4939"/>
  <style name="ruby:regex"                 foreground="#b165d8"/>
  <style name="ruby:simple-interpolation"  foreground="#519f50"/>
  <style name="ruby:special-variable"      foreground="#cc7833" bold="true"/>
  <style name="ruby:symbol"                foreground="#6d9cbe"/>

  <style name="rubyonrails:attribute-definition"  foreground="#da4939" bold="true"/>
  <style name="rubyonrails:block-parameter"       foreground="#8aa6c1" italic="true"/>
  <style name="rubyonrails:builtin"               foreground="#da4939" bold="true"/>
  <style name="rubyonrails:support-constant"      foreground="#da4939"/>
  <style name="rubyonrails:class-inherit"         foreground="#ffffff" italic="true"/>
  <style name="rubyonrails:class-name"            foreground="#DA4939" bold="true"/><!--#8aa6c1-->
  <style name="rubyonrails:class-variable"        foreground="#d0d0ff"/>
  <style name="rubyonrails:complex-interpolation" foreground="#ca88ae"/> <!-- #519f50 -->
  <style name="rubyonrails:constant"              foreground="#da4939"/>
  <style name="rubyonrails:global-variable"       foreground="#ffc66d" bold="true"/>
  <style name="rubyonrails:instance-variable"     foreground="#86aedd"/>
  <style name="rubyonrails:module-handler"        foreground="#cc7833" bold="true"/>
  <style name="rubyonrails:module-name"           foreground="#ffffff" bold="true"/>
  <style name="rubyonrails:predefined-variable"   foreground="#6e9cbe" bold="true"/>
  <style name="rubyonrails:rails"                 foreground="#da4939"/>
  <style name="rubyonrails:regex"                 foreground="#b165d8"/>
  <style name="rubyonrails:simple-interpolation"  foreground="#519f50"/>
  <style name="rubyonrails:special-variable"      foreground="#cc7833" bold="true"/>
  <style name="rubyonrails:symbol"                foreground="#6d9cbe"/>

  <style name="xml:attribute-name"         foreground="#58cdd9"/>
  <style name="xml:doctype"                foreground="#58cdd9"/>
  <style name="xml:element-name"           foreground="#58cdd9"/>
  <style name="xml:namespace"              foreground="#e8bf6a" bold="true"/>
  <style name="xml:tag"                    foreground="#58cdd9"/>

</style-scheme>

